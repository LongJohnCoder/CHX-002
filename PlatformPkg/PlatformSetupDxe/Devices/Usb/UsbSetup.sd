    #ifdef FORM_SET_FORM

    form formid = CHIPSET_SB_USB_FORM_ID,
    title = STRING_TOKEN(STR_USB_FORM);

    subtitle text = STRING_TOKEN(STR_COM_EMPTY);

        oneof varid     = SETUP_DATA.LegacyUsbEnable,
            prompt      = STRING_TOKEN(STR_LEGACY_USB_EN),
            help        = STRING_TOKEN(STR_LEGACY_USB_EN_HELP),
            option text = STRING_TOKEN(STR_DISABLE), value = 0, flags = RESET_REQUIRED;
            option text = STRING_TOKEN(STR_ENABLED), value = 1, flags = RESET_REQUIRED | MANUFACTURING | DEFAULT;
        endoneof;

        oneof varid     = SETUP_DATA.UsbModeSelect,
            prompt      = STRING_TOKEN(STR_USB_MODE),
            help        = STRING_TOKEN(STR_USB_MODE_HELP),
            option text = STRING_TOKEN(STR_DISABLE), value = 0, flags = DEFAULT | MANUFACTURING | RESET_REQUIRED;
            option text = STRING_TOKEN(STR_MODE1),   value = 1, flags = RESET_REQUIRED;
            option text = STRING_TOKEN(STR_MODE2),   value = 2, flags = RESET_REQUIRED;
            option text = STRING_TOKEN(STR_MODE3),   value = 3, flags = RESET_REQUIRED;
            option text = STRING_TOKEN(STR_MODE4),   value = 4, flags = RESET_REQUIRED;
        endoneof;

        suppressif ideqval SETUP_DATA.UsbModeSelect==0 OR ideqval SETUP_DATA.UsbModeSelect==1;
            oneof varid     = SETUP_DATA.XhciMcuDmaPath,
                prompt      = STRING_TOKEN(STR_XHCI_MCU_DMA_PATH),
