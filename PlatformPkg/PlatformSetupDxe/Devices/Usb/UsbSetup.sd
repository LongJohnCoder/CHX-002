#ifdef DEVICES_FORM_SET

    #ifdef FORM_SET_GOTO

    goto CHIPSET_SB_USB_FORM_ID,
    prompt = STRING_TOKEN(STR_USB_FORM),
    help = STRING_TOKEN(STR_USB_FORM_HELP);

