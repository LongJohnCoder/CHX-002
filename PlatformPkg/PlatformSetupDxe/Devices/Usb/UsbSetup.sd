                option text = STRING_TOKEN(STR_STR_XHCI_MCU_DMA_PATH_SNOOP),    value = 1, flags = DEFAULT | MANUFACTURING | RESET_REQUIRED;
            endoneof;
        endif;

        oneof varid     = SETUP_DATA.TRBCacheBypass,
            prompt      = STRING_TOKEN(STR_TRB_CACHE),
            help        = STRING_TOKEN(STR_TRB_CACHE_HELP),
